{
    "files.exclude": {
        "**/.bashrc": true,
        "**/.cache": true,
        "**/.config": true,
        "**/.jupyter": true,
        "**/.local": true,
        "**/.npm": true,
        "**/.yarn": true,
        "**/jupyter_vscode_proxy.egg-info": true
    },
    
    "workbench.colorTheme": "Default Dark Modern",
    "files.autoSaveDelay": 2500,
    "update.mode": "start",
    "telemetry.telemetryLevel": "off",
    "security.workspace.trust.startupPrompt": "always",
    "git.autofetch": true
}
